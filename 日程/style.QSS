QFrame#head{
    background: qlineargradient(x1:0,y1:0,x2:0,y2:1,
                                stop:0 #5CACEE,stop:0.5 #0054FF,stop:1 #5CACEE);
}
QFrame[cate='line_frame']{
    background: rgba(255, 255, 255, 100);
}

QDialog{
    background: qlineargradient(x1:0,y1:0,x2:1,y2:0,
                                stop:0 #5CACEE,stop:1 #5C55EE);
}


QPushButton#finish{
