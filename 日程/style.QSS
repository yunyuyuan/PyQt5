    padding: 0;
    height: 49px;
    width: 49px;
    background-image: url(images/confirm.png);
    background-position: center;
    background-repeat: none;
}
QPushButton#finish:hover{
    background-image: url(images/confirm_hover.png);
}
QPushButton#locate{
    border-radius: 5px;
    padding: 0;
    height: 49px;
    width: 49px;
    background-image: url(images/move.png);
    background-position: center;
    background-repeat: none;
}
QPushButton#locate:hover{
    background-image: url(images/move_hover.png);
}
QPushButton[cate='add']{
    border-radius: 5px;
